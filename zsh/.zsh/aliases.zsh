alias zreload='. ~/.zshrc'

# navigation
alias ls='ls -G'
alias ll='ls -laG'
alias ld='ls -lG | grep "^d"'

alias ..='cd ..'
alias cd..='cd ..'
alias ...='cd ../..'
alias ....='cd ../../..'

#mysql
alias mysql_start='sudo /usr/local/mysql/support-files/mysql.server start'
alias mysql_stop='sudo /usr/local/mysql/support-files/mysql.server stop'
