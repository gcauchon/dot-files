# This is Git's per-user configuration file.
[user]
	name = Guillaume Cauchon
	email = gcauchon@gmail.com
	signingkey = DC13AB49F1B3C51A
[core]
  autocrlf = input
	excludesfile = /Users/gcauchon/.gitignore_global
[commit]
	gpgsign = true
[merge]
	verbosity = 5
	tool = diffmerge
[diff]
	verbosity = 5
	tool = tig
[alias]
	a = add -A
	c = commit -m
	ca = commit --amend
	st = status --untracked-files=all
	ci = commit -v
	br = branch -vv
  bl = branch --list
	co = checkout
	df = diff
  f = fetch
  fp = fetch --prune
	unstage = reset HEAD
	rollback = reset --soft HEAD^1
  r = rebase
	rc = rebase --continue
	rs = rebase --skip
	ra = rebase --abort
	p = push
	ph = push -u origin HEAD
	pf = push -f origin HEAD
